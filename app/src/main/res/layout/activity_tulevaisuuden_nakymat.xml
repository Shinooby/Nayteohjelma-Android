    <EditText
        android:focusable="false"
        android:id="@+id/politiikanVaikutusToimintaanLayout"
        android:layout_margin="10dp"
        android:layout_width="fill_parent"
        android:layout_height="120dp" />
    <TextView
        android:text="Selvit채 keskeisten tuotantopanosten hinnat ja arvioi hintojen kehityst채 tulevaisuudessa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <EditText
        android:focusable="false"
        android:id="@+id/tuotantoPanostenHinnatEditText"
        android:layout_margin="10dp"
        android:layout_width="fill_parent"
        android:layout_height="120dp" />
</LinearLayout>
