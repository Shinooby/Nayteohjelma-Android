<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="siirtyminenfragmenteilla.app.yritystoiminnan_alisivut.YllapitoJaKehittaminen">

    <TextView
        android:text="Miten yrityksessä tarkkaillaan tuotantoa ja kuinka saatuja tietoja käytetään hyväksi?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <EditText
        android:focusable="false"
        android:id="@+id/tuotannonTarkkailuEditText"
        android:layout_margin="10dp"
        android:layout_width="fill_parent"
        android:layout_height="120dp" />
    <TextView
        android:text="Kuka hoitaa yrityksen kirjanpidon ja miten kirjanpitoa hyödynnetään päätöksenteossa?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <EditText
        android:focusable="false"
        android:id="@+id/kirjanpidonHoitoEditText"
        android:layout_margin="10dp"
        android:layout_width="fill_parent"
        android:layout_height="120dp" />
    <TextView
        android:text="Miten yrittäjä kehittää yritystä ja omaa osaamistaan?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <EditText
        android:focusable="false"
        android:id="@+id/yrityksenKehittaminenEditText"
        android:layout_margin="10dp"
        android:layout_width="fill_parent"
        android:layout_height="120dp" />
</LinearLayout>
